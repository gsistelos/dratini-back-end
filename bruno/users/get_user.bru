meta {
  name: get_user
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/users/{{userId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.id: isDefined
  res.body.username: isDefined
  res.body.email: isDefined
  res.body.createdAt: isDefined
  res.body.updatedAt: isDefined
  res.body.password: isUndefined
}
